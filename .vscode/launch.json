    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug sccache client",
            "program": "${workspaceFolder}/target/x86_64-unknown-linux-musl/debug/sccache",
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {
                "SCCACHE_LOG": "sccache=debug",
                "SCCACHE_ERROR_LOG": "/tmp/sccache_debug.log",
                "SCCACHE_SERVER_PORT": "4226",
                "SCCACHE_NO_DAEMON": "1",
                "SCCACHE_START_SERVER": "1",
                "SCCACHE_IDLE_TIMEOUT": "0",
                "SCCACHE_CONF": "${workspaceFolder}/.devcontainer/.config/sccache/client.conf"
            },
            "stdio": null,
            "terminal": "console",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'sccache'",
            "cargo": {
                "args": [
                    "test",
                    "-F",
                    "all,dist-server,vendored-openssl",
                    "--no-run",
                    "--lib",
                    "--package", "sccache"
                ],
                "filter": {
                    "name": "sccache",
                    "kind": "lib"
                }
            },
            "args": ["test_parse_threads_argument_simple_cu"],
            "cwd": "${workspaceFolder}"
        },
        {
